!!!
%html{:lang => "en"}
  %head
    %title StaunchRobots
    %meta{:content => "text/html; charset=utf-8", "http-equiv" => "Content-Type"}/
    %script{:src => "/javascripts/jquery_1_7_2.js", :type => "text/javascript"}
    %script{:src => "/javascripts/jquery.anythingslider.js", :type => "text/javascript"}
    %script{:src => "/javascripts/scripts.js", :type => "text/javascript"}
    %script{:src => "http://use.typekit.com/cyd0urf.js", :type => "text/javascript"}
    :javascript
      try{Typekit.load();}catch(e){}
      $(function() {
      $(".commits_btn").click(function(e) {
      var commits = $(this).parents('.team-box').find('.commits');
      if (commits.is(":visible")) {
        $(this).parents('.team-box').find('.commits').slideUp('fast');
      } else {
        $(this).parents('.team-box').find('.commits').slideDown('fast');
      }
      e.preventDefault();
      })
      $('.commits').hide();
      })
    %link{:href => "/css/style.css", :media => "all", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "/css/anythingslider.css", :media => "all", :rel => "stylesheet", :type => "text/css"}/
